{
  "version": "0.1",
  "resources": {
    "sessions": {
      "name": "session",
      "class": "besluit:Zitting",
      "attributes": {
        "started-at": {
          "type": "string",
          "predicate": "prov:startedAtTime"
        },
        "ended-at": {
          "type": "string",
          "predicate": "prov:endedAtTime"
        }
      },
      "relationships": {
        "governing-agent": {
          "predicate": "besluit:isGehoudenDoor",
          "target": "governing-agent",
          "cardinality": "one"
        }
			},
      "new-resource-base": "http://dev.bnb.lblod.info/sessions"
    }
  }
}
