{
  "version": "0.1",
  "resources": {
    "agenda-items": {
      "name": "agendaItem",
      "class": "besluit:Agendapunt",
      "attributes": {
        "title": {
          "type": "string",
          "predicate": "dct:title"
        },
        "description": {
          "type": "string",
          "predicate": "dct:description"
        },
        "planned-for-public": {
          "type": "boolean",
          "predicate": "besluit:geplandOpenbaar"
        },        
        "public": {
          "type": "boolean",
          "predicate": "besluit:openbaar"
        },
        "alternate-link" : {
          "type": "string",
          "predicate": "owl:sameAs"
        }
      },
      "relationships": {
        "generated": {
          "predicate": "prov#generated",
          "target": "decision",
          "cardinality": "one"
        },
        "session": {
          "predicate": "besluit:behandelt",
          "target": "session",
          "cardinality": "one",
          "inverse": true
        },
        "handled": {
          "predicate": "dct:subject",
          "target": "handledAgendaItem",
          "cardinality": "one",
          "inverse": true
        }
      },
      "new-resource-base": "http://dev.bnb.lblod.info/agenda-items/"
    }
  }
}
